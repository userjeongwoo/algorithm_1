package Baekjoon;

import java.util.Scanner;

public class Test_10810 {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int N,M;
		N=sc.nextInt(); //바구니 개수
		int[] arr=new int[N]; //바구니 생성
		M=sc.nextInt();	//공을 넣을 횟수
		
		for(int o=0;o<M;o++) {
			int i,j,k;
			i=sc.nextInt();
			j=sc.nextInt();
			k=sc.nextInt();
			for (int l=i;l<=j;l++) {
				arr[l-1]=k;
			}
		}
		for (int i=0;i<arr.length;i++) {
			System.out.print(arr[i]+" ");
		}
		
	}

}
